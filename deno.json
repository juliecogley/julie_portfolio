{
  "imports": {
    "lume/": "https://deno.land/x/lume@v2.3.3/",
    // "lume/cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@v0.7.0/"
    "lume/cms/": "https://cdn.jsdelivr.net/gh/lumeland/cms@1d2ca5d95f33ff6b223667f2f02f3070b5ec0e76/"
  },
  "tasks": {
    "lume": "export TZ='Asia/Tokyo' && echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s",
    "cms": "deno task lume cms"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
